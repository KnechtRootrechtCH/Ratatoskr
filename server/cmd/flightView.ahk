CycleCameraView() {
    Send {F4}
}
CycleCameraOrbitMode() {
    Send {F3}
}
ZoomIn() {
    Send {WheelUp}
}
ZoomOut() {
    Send {WheelDown}
}
Freelook_Toggle() {
    Send z
}
DynamicZoomIn() {
    Send {WheelUp}
}
DynamicZoomOut() {
