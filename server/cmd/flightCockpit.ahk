Eject() {
    Send {LAlt down}
    Send l
    Sleep 100
    Send l
    Send {LAlt up}
}
ExitSeat() {
    Send {LAlt down}
    Send f
    Send {LAlt up}
}
SelfDestruct() {
    Send {LAlt down}
    Send {BackSpace}
    Send {LAlt up}
}
CoolerRateIncrease() {
    Send {LAlt down}
    Send {Numpad8}
    Send {LAlt up}
}
CoolerRateDecrease() {
    Send {LAlt down}
    Send {Numpad7}
    Send {LAlt up}
}
FlightReady() {
    Send 8
}
DoorsOpen() {
    Send 9
}
DoorsClose() {
    Send 9
}
DoorsLock() {
    Send 0
}
DoorsUnlock() {
    Send 0
}